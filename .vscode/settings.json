{
  "ndjson.port": 9001,
  "claudeCodeChat.permissions.yoloMode": true,
  "docker.showStartPage": false,
  "docker.defaultRegistryPath": "",
  "docker.composeBuild": {
    "detach": true,
    "progress": "plain"
  },
  "docker.composeUp": {
    "detach": true,
    "files": [
      "docker-compose.yml",
      "docker-compose.backtest.yml"
    ]
  },
  "docker.containers.label": "ContainerName",
  "docker.images.label": "Tag",
  "docker.volumes.label": "Name",
  "docker.networks.label": "Name",
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true
  },
  "azureFunctions.scmDoBuildDuringDeployment": true,
  "azureFunctions.pythonVenv": ".venv",
  "azureFunctions.projectLanguage": "Python",
  "azureFunctions.projectRuntime": "~4",
  "debug.internalConsoleOptions": "neverOpen",
  "containers.containers.label": "ContainerName",
  "containers.images.label": "Tag",
  "containers.networks.label": "Name",
  "containers.volumes.label": "Name",
  "liveServer.settings.port": 5501,
  "azureFunctions.projectSubpath": "azure\\function_app",
  "chat.agent.maxRequests": 250,
  "remote.autoForwardPortsSource": "process",
  "remote.restoreForwardedPorts": true,
  "remote.SSH.connectTimeout": 300
}
