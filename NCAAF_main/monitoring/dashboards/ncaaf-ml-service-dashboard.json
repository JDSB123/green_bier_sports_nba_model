{
  "dashboard": {
    "title": "NCAAF v5.0 - ML Service",
    "tags": ["ncaaf", "ml", "predictions", "python"],
    "timezone": "browser",
    "schemaVersion": 38,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "System Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "ncaaf_ml_system_uptime_seconds",
            "legendFormat": "Uptime"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 3600, "color": "yellow"},
                {"value": 86400, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Predictions Generated",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(rate(ncaaf_ml_predictions_total[5m]))",
            "legendFormat": "Predictions/sec"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Models Loaded",
        "type": "stat",
        "targets": [
          {
            "expr": "ncaaf_ml_models_loaded",
            "legendFormat": "Models"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 4, "color": "green"}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Recommended Bets",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(ncaaf_ml_recommended_bets_total)",
            "legendFormat": "Total Recommended"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Prediction Duration by Model (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ncaaf_ml_prediction_duration_seconds_bucket[5m])) by (le, model_type))",
            "legendFormat": "{{model_type}}"
          }
        ],
        "yaxes": [
          {"format": "s"},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 6,
        "title": "Feature Extraction Duration (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ncaaf_ml_feature_extraction_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(ncaaf_ml_feature_extraction_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          }
        ],
        "yaxes": [
          {"format": "s"},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 7,
        "title": "Prediction Confidence Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(ncaaf_ml_prediction_confidence_bucket[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      },
      {
        "id": 8,
        "title": "Predictions with Edge",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ncaaf_ml_predictions_with_edge_total{bet_type=\"spread\"}[5m])",
            "legendFormat": "Spread"
          },
          {
            "expr": "rate(ncaaf_ml_predictions_with_edge_total{bet_type=\"total\"}[5m])",
            "legendFormat": "Total"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12}
      },
      {
        "id": 9,
        "title": "API Request Duration (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ncaaf_ml_api_request_duration_seconds_bucket[5m])) by (le, endpoint))",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "yaxes": [
          {"format": "s"},
          {"show": false}
        ],
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 20}
      },
      {
        "id": 10,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ncaaf_ml_cache_hits_total[5m]) / (rate(ncaaf_ml_cache_hits_total[5m]) + rate(ncaaf_ml_cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %"
          }
        ],
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"show": false}
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 20}
      },
      {
        "id": 11,
        "title": "Database Query Duration (P95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(ncaaf_ml_db_query_duration_seconds_bucket[5m])) by (le, operation))",
            "legendFormat": "{{operation}}"
          }
        ],
        "yaxes": [
          {"format": "s"},
          {"show": false}
        ],
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 26}
      },
      {
        "id": 12,
        "title": "Errors by Component",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(ncaaf_ml_errors_total[5m])",
            "legendFormat": "{{component}} - {{error_type}}"
          }
        ],
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 26}
      },
      {
        "id": 13,
        "title": "Recommended Bets by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (bet_type) (ncaaf_ml_recommended_bets_total)",
            "legendFormat": "{{bet_type}}"
          }
        ],
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 32}
      },
      {
        "id": 14,
        "title": "API Response Status Codes",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (status) (rate(ncaaf_ml_api_requests_total[5m]))",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 6, "w": 16, "x": 8, "y": 32}
      }
    ]
  }
}
